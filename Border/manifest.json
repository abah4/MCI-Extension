{

  "description": " Border around the web page when button is clicked",
  "manifest_version": 2,
  "name": "button-demo",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": {
      "16": "icons/page-16.png",
      "32": "icons/page-32.png"
    }
  },

  "permissions": [
    "activeTab",
    "tabs"
  ]

}
